    <div class="container-fluid">
        <div class="dashboard-container">
            <div>
                <h4 style="color:white;">@ViewBag.Heading</h4>
            </div>
            <div class="dashboard-wrapper">
                <div class="left-sidebar">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript">
        $("#overlay").delay(1250).fadeOut(500);
        var source = new EventSource('/Notifications/CheckNotification');
        source.onmessage = function (e) {
            //alert(e);
            var returnedItem = JSON.parse(e.data);
            updateDashboard(returnedItem.orders, returnedItem.requests);
        }

        function updateDashboard(orders, requests) {
            $('#waitingPricingOrders').html('<span class="info-label badge badge-warning">' + orders[0] +
                    '</span>');
            $('#waitingPricingRequests').html('<span class="info-label badge badge-info">' + orders[1] +
                    '</span>');
            $('#approvedRequests').html('<span class="info-label badge badge-inverse">' + orders[2] +
                    '</span>');

            $('#collectedOrders').html('<span class="info-label badge badge-important">' + requests[1] +
                    '</span>');

                $('#OrdersNeedToApprove').html(orders[0]);
                $('#OrdersApproved').html(orders[1]);
                $('#OrdersRejected').html(orders[2]);
                $('#OrdersRequested').html(orders[3]);
                $('#OrdersDelivering').html(orders[4]);
                $('#OrdersWaitingForReturn').html(orders[5]);
                $('#OrdersReturned').html(orders[6]);

                $('#RequestsWaitingForPrice').html(requests[0]);
                $('#RequestsNeedToApprove').html(requests[1]);
                $('#RequestsApproved').html(requests[2]);
                $('#RequestsCollected').html(requests[3]);
                $('#RequestsCanceled').html(requests[4]);
            
        }
    </script>

