            </li>
            <li>
                <a href="/Customer/Requests?&status=pricing">
                    <div class="fs1" aria-hidden="true" data-icon="&#xe038;"></div>
                    <div id="collectedOrders">
                        <span class="info-label badge badge-important">9
                        </span>
                    </div>
                </a>
            </li>
        </ul>

