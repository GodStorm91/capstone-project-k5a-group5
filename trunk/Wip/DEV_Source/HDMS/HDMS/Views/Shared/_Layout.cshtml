    <script type="text/javascript">
        //adding load class to body and hide page
        document.documentElement.className += 'loadstate';

        $(document).ready(function () {
            var block = function () {
                $.blockUI({ message: 'processing...' });
                $('.modal').block({ message: 'processing...' });
            };
            var unblock = function () {
                $.unblockUI();
                $('.modal').unblock();                               
            };
            //$(document).ajaxStart(block).ajaxStop(unblock);                       

            $('.myToolTip').tooltip();

            $("#alert").hide();
        });

        function alert(textToshow,link, timeToShow) {
            $("#alertText").html(textToshow);
            document.getElementById("alertLink").href = link;
            $("#alert").fadeIn(1000, function () {
                $("#alertText").fadeIn(700);
            });
            setTimeout(function () {
                hideAlert();
            }, timeToShow);
        }

        function hideAlert() {
            //$("#alert").fadeOut(700);
        }

    </script>

